
@article{Lin2014281,
title = "Signal characteristics of a resistive-strip micromegas detector with an integrated two-dimensional readout ",
journal = "Nuclear Instruments and Methods in Physics Research Section A: Accelerators, Spectrometers, Detectors and Associated Equipment ",
volume = "767",
number = "",
pages = "281 - 288",
year = "2014",
note = "",
issn = "0168-9002",
doi = "http://dx.doi.org/10.1016/j.nima.2014.09.002",
url = "http://www.sciencedirect.com/science/article/pii/S0168900214009942",
author = "Tai-Hua Lin and Andreas Düdder and Matthias Schott and Chrysostomos Valderanis and Laura Wehner and Robert Westenberger",
keywords = "Microstructure detector",
keywords = "Micromegas",
keywords = "Two-dimensional readout",
keywords = "Gaseous detector ",
abstract = "Abstract In recent years, micropattern gaseous detectors, which comprise a two-dimensional readout structure within one \{PCB\} layer, received significant attention in the development of precision and cost-effective tracking detectors in medium and high energy physics experiments. In this paper, we present for the first time a systematic performance study of the signal characteristics of a resistive strip micromegas detector with a two-dimensional readout, based on test-beam and X-ray measurements. In particular, comparisons of the response of the two independent readout layers regarding their signal shapes and signal reconstruction efficiencies are presented. "
}

